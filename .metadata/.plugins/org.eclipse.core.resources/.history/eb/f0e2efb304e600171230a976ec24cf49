
public class Person {

	public String name=null;
	public int age=0;

	public String getNme(){
	return this.name;
	}

	public void setName(String name){
		this.name = name;
	}

	public Person(String name,int age){
	this.name=name;
	int.age=age;
	}

	public Person(int age,String name){
		this.name=name;
		this.age=age;
	}

}
